<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  displayBlock="true">
  <Table id="table" items="{/ListOfIncidents}">
    <columns>
      <Column><Text text="ID"/></Column>
      <Column><Text text="Title"/></Column>
      <Column><Text text="Customer"/></Column>
    </columns>
    <items>
      <ColumnListItem>
        <cells>
          <Text text="{ID}"/>
          <Text text="{title}"/>
          <Text text="{customer_ID}"/>
        </cells>
      </ColumnListItem>
    </items>
  </Table>
</mvc:View>
